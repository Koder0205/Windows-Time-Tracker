<Window x:Class="WinTimeTracker.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WinTimeTracker"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="400" WindowStyle="None" ResizeMode="NoResize">
    <Grid x:Name="MainGrid" Background="Black">
        
        <Button HorizontalAlignment="Right" Margin="0,0,0,0" VerticalAlignment="Top" Height="32" Width="32" Visibility="Visible">
            <Image Source="Resources/white_delete_32px.png"/>
        </Button>
    </Grid>
</Window>
